{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "parcel": "node_modules/parcel-bundler/bin/cli.js watch app/index.html --out-dir app-dist/ --public-url /public/",
    "parcel-build": "parcel build app/index.html --out-dir app-dist/ --public-url /public/"
  },
  "dependencies": {
    "@date-io/core": "1.3.11",
    "@date-io/moment": "1.3.11",
    "@material-ui/core": "4.5.0",
    "@material-ui/icons": "4.4.3",
    "@material-ui/pickers": "3.2.6",
    "@material-ui/styles": "4.5.0",
    "@nestjs/common": "6.0.4",
    "@nestjs/core": "6.0.4",
    "@nestjs/microservices": "6.0.4",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/serve-static": "1.0.1",
    "@nestjs/websockets": "6.0.4",
    "@sentry/browser": "5.6.3",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "history": "4.10.1",
    "i18n-iso-countries": "4.3.1",
    "i18next": "17.0.18",
    "i18next-icu": "1.1.2",
    "moment": "2.24.0",
    "mysql2": "1.6.5",
    "prop-types": "15.7.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-dropzone": "10.1.9",
    "react-ga": "2.6.0",
    "react-hot-loader": "4.12.14",
    "react-i18next": "10.13.1",
    "react-infinite-scroller": "1.2.4",
    "react-material-ui-form-validator": "2.0.9",
    "react-redux": "7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.1",
    "redux-promise-middleware": "5.1.1",
    "redux-saga": "1.1.1",
    "redux-thunk": "2.3.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "sequelize": "5.12.2",
    "sequelize-cli": "5.5.0",
    "sequelize-typescript": "0.6.11",
    "socket.io-client": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/sequelize": "4.28.4",
    "@types/supertest": "2.0.7",
    "concurrently": "4.1.0",
    "jest": "24.7.1",
    "nodemon": "1.18.11",
    "parcel-bundler": "1.12.3",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
